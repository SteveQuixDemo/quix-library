{
    "libraryItemId": "1bb04ae0-2c62-490a-ae77-8a7af00a8853",
    "name": "Google Pub/Sub Connector",
    "language": "Python",
    "tags": {
        "Complexity": ["Medium"],
		"SDK Usage": ["Write SDK"],
        "Pipeline Stage": ["Source"]
    },
    "shortDescription": "Connect your Google Pub/Sub topic to a Quix topic with this bridge",
    "longDescription": "If you already have a Google Pub/Sub topic, you can connect it to Quix with this simple bridge.",
    "DefaultFile": "quix_functions.py",
	"EntryPoint": "build/dockerfile",
    "RunEntryPoint": "main.py",
    "Variables": [
        {
			"Name" : "output",
            "Type" : "EnvironmentVariable",
            "InputType" : "OutputTopic",
            "Description" : "This is the Quix Topic that will receive the stream",
            "DefaultValue" : "google-pub-sub-data",
            "Required": true
        },
        {
            "Name" : "google_key",
            "Type" : "EnvironmentVariable",
            "InputType" : "FreeText",
            "Description" : "Google Service Account Credentials",
            "DefaultValue" : "",
            "Required": true
        },
        {
            "Name" : "google_project_id",
            "Type" : "EnvironmentVariable",
            "InputType" : "HiddenText",
            "Description" : "The ID of the Google project",
            "DefaultValue" : "",
            "Required": true
        },
        {
            "Name" : "google_subscription_id",
            "Type" : "EnvironmentVariable",
            "InputType" : "FreeText",
            "Description" : "The ID of the Google subscription",
            "DefaultValue" : "",
            "Required": true
        }
    ],
	"DeploySettings":
	{
		"DeploymentType": "Job",
		"CpuMillicores": 200,
		"MemoryInMb": 200,
		"Replicas": 1,
		"PublicAccess": false,
		"ValidateConnection": true
	}
}
